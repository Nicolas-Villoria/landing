#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nicolasvilloria/Desktop/DATOS/projects/docweb/node_modules/.pnpm/astro@5.6.1_jiti@2.4.2_lightningcss@1.29.2_rollup@4.39.0_typescript@5.8.3/node_modules/astro/node_modules:/Users/nicolasvilloria/Desktop/DATOS/projects/docweb/node_modules/.pnpm/astro@5.6.1_jiti@2.4.2_lightningcss@1.29.2_rollup@4.39.0_typescript@5.8.3/node_modules:/Users/nicolasvilloria/Desktop/DATOS/projects/docweb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nicolasvilloria/Desktop/DATOS/projects/docweb/node_modules/.pnpm/astro@5.6.1_jiti@2.4.2_lightningcss@1.29.2_rollup@4.39.0_typescript@5.8.3/node_modules/astro/node_modules:/Users/nicolasvilloria/Desktop/DATOS/projects/docweb/node_modules/.pnpm/astro@5.6.1_jiti@2.4.2_lightningcss@1.29.2_rollup@4.39.0_typescript@5.8.3/node_modules:/Users/nicolasvilloria/Desktop/DATOS/projects/docweb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.6.1_jiti@2.4.2_lightningcss@1.29.2_rollup@4.39.0_typescript@5.8.3/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.6.1_jiti@2.4.2_lightningcss@1.29.2_rollup@4.39.0_typescript@5.8.3/node_modules/astro/astro.js" "$@"
fi
